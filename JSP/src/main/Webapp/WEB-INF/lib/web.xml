<?xml version=1.0 encoding=UTF-8?>
<web-app xmlns=http://xmlns.jcp.org/xml/ns/javaee
         xmlns:xsi=http://www.w3.org/2001/XMLSchema-instance
         xsi:schemaLocation=http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd
         version=3.1>
    <display-name>Sublet01</display-name>
    <welcome-file-list>
        <welcome-file>/250324.jsp</welcome-file>
        <welcome-file>/250324_formed.jsp</welcome-file>
        <welcome-file>/250324_inspect.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>250324</servlet-name>
        <servlet-class>global.Sublet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>250324</servlet-name>
        <url-pattern>/src/main/webapp/250324.jsp</url-pattern>
    </servlet-mapping>
    <display-name>Login</display-name>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>global.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/src/main/webapp/250324_formed.jsp</url-pattern>
    
    <display-name>Sublet02</display-name>
    <servlet>
        <servlet-name>250324-2</servlet-name>
        <servlet-class>global.Sublet02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>250324-2</servlet-name>
        <url-pattern>/src/main/webapp/250324_inspect</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/src/main/webapp/404.jsp</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/src/main/webapp/500.jsp</location>
    </error-page>
</web-app>